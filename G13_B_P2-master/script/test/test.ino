void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
}
int d_time = 500;
void loop() {

  // put your main code here, to run repeatedly:
  Serial.println("Distance: 5.0 angle: 0.0");
  delay(d_time);
  Serial.println("Distance: 5.0 angle: 0.0");
  delay(d_time);


  Serial.println("Distance: 0.0 angle: 45.0");
  delay(d_time);
  Serial.println("Distance: 0.0 angle: -45.0");
  delay(d_time);

  Serial.println("Distance: 0.0 angle: 45.0");
  delay(d_time);





  Serial.println("Distance: 10 angle: 0");
  delay(d_time);
  Serial.println("Distance: 0 angle: 135");
  delay(d_time);
  Serial.println("Distance: 0.0 angle: -45.0");
  delay(d_time);

  Serial.println("Distance: 0.0 angle: 45.0");
  delay(d_time);



  Serial.println("Distance: 17.07106 angle: 0");
  delay(d_time);


  Serial.println("Distance: 0 angle: 90");
  delay(d_time);
  Serial.println("Distance: 0.0 angle: -45.0");
  delay(d_time);

  Serial.println("Distance: 0.0 angle: 45.0");
  delay(d_time);


  Serial.println("Distance: 7.07106 angle: 0");
  delay(d_time);

  Serial.println("Distance: 0 angle: 90");
  delay(d_time);



  delay(d_time * 10);
}


